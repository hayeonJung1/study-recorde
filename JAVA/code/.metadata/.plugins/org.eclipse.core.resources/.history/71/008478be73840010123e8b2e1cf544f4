package mypack;

public class Test {
	public static void main(String[] args) {
		int a1=3;
		int b1=++a1;	// a1값을 1증가 -> a1 값을 b1에 대입
		System.out.println(a1); //4
		System.out.println(b1); //4
		
		int a2=3;
		int b2=a1++;    // a2 값을 b2에 대입 -> a2 값을 1증가
		System.out.println(a2); //4
		System.out.println(b2); //3
		
	}

}
